{
  "name": "react-electron-starter",
  "version": "0.1.0",
  "description": "Lectures application",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development HOT=1 electron main.js",
    "prod": "cross-env NODE_ENV=production HOT=0 electron main.js",
    "dev": "cross-env NODE_ENV=development HOT=1 node server.js",
    "postinstall": "webpack -p --config webpack.config.js",
    "package": "npm run postinstall && build --publish never",
    "package-win": "npm run postinstall && build --win --x64",
    "package-linux": "npm run postinstall && build --linux",
    "package-all": "npm run postinstall && build -mwl"
  },
  "author": "denis",
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "asar": "^0.13.0",
    "assert": "^1.4.1",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.7",
    "classnames": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.23.1",
    "electron-builder": "^12.3.1",
    "electron-packager": "^8.5.1",
    "electron-prebuilt": "^0.37.0",
    "express": "^4.13.4",
    "file-loader": "^0.10.0",
    "hash-files": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.0.0",
    "json-loader": "^0.5.4",
    "keymirror": "^0.1.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "moment": "^2.15.1",
    "node-sass": "^3.4.2",
    "object-path": "^0.9.2",
    "react": "^15.0.1",
    "react-bootstrap": "^0.30.2",
    "react-dom": "^15.0.1",
    "react-if": "^2.1.0",
    "react-intl": "^2.1.2",
    "react-loading": "0.0.9",
    "react-router": "^2.4.0",
    "react-router-transition": "0.0.6",
    "rsvp": "^3.2.1",
    "sass-loader": "^3.2.0",
    "sprintf-js": "^1.0.3",
    "style-loader": "^0.13.1",
    "temp": "^0.8.3",
    "underscore": "^1.8.3",
    "underscore.path": "^0.1.4",
    "underscore.string": "^3.3.4",
    "url-loader": "^0.5.7",
    "validate.js": "^0.9.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "build": {
    "productName": "ElectronReact",
    "appId": "org.develar.ElectronReact",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "main.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  }
}
